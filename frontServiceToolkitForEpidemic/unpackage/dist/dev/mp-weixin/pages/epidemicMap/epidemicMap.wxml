<view class="map"><view class="selection-div"><text class="title1">疫情地图</text><view class="input-box"><picker mode="selector" range-key="name" value="{{index}}" range="{{itemList}}" data-event-opts="{{[['change',[['getItemSelected',['$event']]]]]}}" bindchange="__e"><label>{{itemList[index].name}}</label></picker></view></view><view style="width:100%;height:400px;margin:0 auto;"><view data-ref="mapbox" class="map-img _div vue-ref"></view></view><view class="outlineData"><view class="block-item"><view class="block"><text class="title">现存确诊</text><text class="data">{{this.currentConfirmedCount}}</text><view class="trendData"><text class="trend">较昨日增加</text><text class="incr">{{this.currentConfirmedIncr}}</text></view></view><view class="block"><text class="title">累计确诊</text><text class="data" style="color:#af2424;">{{this.confirmedCount}}</text><view class="trendData"><text class="trend">较昨日增加</text><text class="incr">{{this.confirmedIncr}}</text></view></view></view><view class="block-item"><view class="block"><text class="title">累计死亡</text><text class="data" style="color:#000000;">{{this.deadCount}}</text><view class="trendData"><text class="trend">较昨日增加</text><text class="incr">{{this.deadIncr}}</text></view></view><view class="block"><text class="title">累计治愈</text><text class="data" style="color:#309f0e;">{{this.curedCount}}</text><view class="trendData"><text class="trend">较昨日增加</text><text class="incr">{{this.curedIncr}}</text></view></view></view></view><view class="map-list"><view data-event-opts="{{[['tap',[['goCommunity',['$event']]]]]}}" class="map-list-item border-bottom" bindtap="__e"><text class="list-icon"></text><text class="list-text">小区疫情查询</text><text class="navigat-arrow"></text></view><view data-event-opts="{{[['tap',[['goGraph',['$event']]]]]}}" class="map-list-item" bindtap="__e"><text class="list-icon"></text><text class="list-text">全国/世界疫情趋势图</text><text class="navigat-arrow"></text></view></view></view>